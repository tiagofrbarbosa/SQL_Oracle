SELECT *
FROM atendime
WHERE DT_ATENDIMENTO BETWEEN '03-07-2014' AND '04-07-2014'
AND CD_PACIENTE = 205201
AND CD_MULTI_EMPRESA = 1

SELECT NM_PACIENTE FROM PACIENTE WHERE CD_PACIENTE = 205201

SELECT * FROM ORI_ATE WHERE CD_ORI_ATE = 13

SELECT * FROM TIP_RES WHERE cd_tip_res = 31

SELECT * FROM PRESTADOR WHERE CD_PRESTADOR = 1401

SELECT * FROM SER_DIS WHERE CD_SER_DIS = 52

SELECT *
FROM ATENDIME A
WHERE A.DT_ATENDIMENTO BETWEEN '03-04-2014' AND '4-04-2014'
AND A.CD_SER_DIS = 52
--AND A.CD_PACIENTE = 574541

SELECT * FROM ATENDIME
SELECT * FROM TIP_MAR

SELECT A.CD_ATENDIMENTO,
       To_Char(A.DT_ATENDIMENTO,'DD/MM/YYYY'),
       P.NM_PACIENTE,
       P.CD_PACIENTE,
       RP.DS_RESPOSTA,
       LP.DS_LOC_PROCED
       FROM ATENDIME A,
            PACIENTE P,
            VDIC_RESPOSTA_DOCUMENTO VRD,
            REGISTRO_RESPOSTA RP,
            LOC_PROCED LP
       WHERE A.DT_ATENDIMENTO BETWEEN '10-04-2014' AND '11-04-2014'
       AND A.CD_ATENDIMENTO = VRD.ATENDIMENTO
       AND VRD.REGISTRO_DOCUMENTO = RP.CD_REGISTRO_DOCUMENTO
       AND A.CD_PACIENTE = P.CD_PACIENTE
       AND A.CD_SER_DIS = 52
       AND RP.CD_PERGUNTA_DOC = 6247
       AND A.CD_LOC_PROCED = LP.CD_LOC_PROCED
       ORDER BY 1

SELECT P.NM_PACIENTE,
       INI.CD_ATENDIMENTO,
       INI.DT_ATENDIMENTO,
       FIN.DT_ALTA_MEDICA
FROM ATENDIME FIN, ATENDIME INI, PACIENTE P, PRESTADOR PR
WHERE FIN.DT_ALTA_MEDICA BETWEEN '01-03-2014' AND '31-03-2014'
AND FIN.CD_PACIENTE = P.CD_PACIENTE
AND INI.CD_PACIENTE = P.CD_PACIENTE
AND FIN.CD_ESPECIALID = 55
AND INI.CD_ESPECIALID = 55
AND FIN.CD_TIP_RES = 31
AND FIN.CD_MULTI_EMPRESA = 1
AND INI.CD_MULTI_EMPRESA = 1
AND INI.CD_TIP_MAR = 1
AND INI.CD_TIP_RES = 28
AND INI.CD_SER_DIS = 52
AND INI.CD_PRESTADOR = 1401
AND FIN.CD_PRESTADOR = 1401
AND INI.CD_PACIENTE = FIN.CD_PACIENTE
AND FIN.CD_PRESTADOR = PR.CD_PRESTADOR
ORDER BY FIN.DT_ALTA_MEDICA



SELECT * FROM ATENDIME
WHERE CD_ATENDIMENTO = 2436706

SELECT * FROM ATENDIME WHERE CD_PACIENTE = 107558 ORDER BY DT_ATENDIMENTO
SELECT * FROM ESPECIALID WHERE CD_ESPECIALID = 55
SELECT * FROM TIP_RES WHERE CD_TIP_RES = 31

SELECT * FROM ATENDIME WHERE DT_ATENDIMENTO BETWEEN '07-03-2014' AND '08-03-2014' AND CD_SER_DIS = 52
SELECT * FROM PACIENTE WHERE CD_PACIENTE = 1005477

SELECT * FROM IT_AGENDA_CENTRAL WHERE CD_PACIENTE = 242330 ORDER BY 1

SELECT * FROM VDIC_RESPOSTA_DOCUMENTO WHERE ATENDIMENTO = 2735683
SELECT * FROM DOCUMENTO WHERE CD_DOCUMENTO = 100
SELECT * FROM REGISTRO_RESPOSTA WHERE CD_REGISTRO_DOCUMENTO =  2597858

SELECT * FROM FILA_SENHA WHERE CD_MULTI_EMPRESA =1 ORDER BY 1;


SELECT P.NM_PACIENTE
  FROM PACIENTE P
        WHERE P.CD_PACIENTE IN(

        SELECT P.CD_PACIENTE,
               p.NM_PACIENTE
          FROM PACIENTE P,
               ATENDIME A
        WHERE P.CD_PACIENTE = A.CD_PACIENTE
        AND A.DT_ALTA_MEDICA BETWEEN '01-03-2014' AND '30-03-2014'
        AND A.CD_ESPECIALID = 55
        AND A.CD_TIP_RES = 31
        AND A.CD_MULTI_EMPRESA = 1
        AND A.CD_PRESTADOR = 1401
        ORDER BY 1
        )